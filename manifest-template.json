{
  "name": "Purdue Auto Login",
  "description": "Bypass Purdue Login's 2-factor authentication.",
  "options_ui": {
    "open_in_tab": true,
    "page": "options.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://api-1b9bef70.duosecurity.com/"
  ],
  "content_scripts": [
    {
      "matches": ["https://sso.purdue.edu/*"],
      "js": ["ssoPageInjection.js"]
    },
    {
      "matches": ["https://api-1b9bef70.duosecurity.com/frame/v4/auth/*"],
      "js": ["duoPageInjection.js"]
    }
  ],
  "manifest_version": 3
}
